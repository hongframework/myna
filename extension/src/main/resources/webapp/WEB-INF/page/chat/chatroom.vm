<div id="chat-item-me-template" class="ng-scope" style="display:none">
    <div class="clearfix">

        <div ng-switch="" on="message.MsgType" style="overflow: hidden;" ng-init="contact = getUserContact(message.MMActualSender,message.MMPeerUserName);">

            <div ng-switch-default="" class="message ng-scope me" ng-class="{&quot;you&quot;:!message.MMIsSend,&quot;me&quot;:message.MMIsSend,&quot;first&quot;:($index==0&amp;&amp;!message.MMTime)}">
                <!-- ngIf: message.MMTime||(newMsg && newMsg==message) -->

                <div ng-if="message.MMTime||(newMsg &amp;&amp; newMsg==message)" class="message_system ng-scope"><!-- ngIf: (isScrollToUnread && newMsg==message) --><!-- ngIf: (message.MMTime&&!(newMsg && newMsg==message)) -->
                    <div ng-if="(message.MMTime&amp;&amp;!(newMsg &amp;&amp; newMsg==message))" class="content ng-binding ng-scope">16:10</div><!-- end ngIf: (message.MMTime&&!(newMsg && newMsg==message)) -->
                </div><!-- end ngIf: message.MMTime||(newMsg && newMsg==message) -->

                <img class="avatar" src=""  title="我" ng-click="showProfile($event,message.MMActualSender)">
                <div class="content">
                    <h4 class="nickname ng-binding ng-scope" style="padding-right:  10px; width: auto;">我</h4>
                    <span class="desc ng-scope  ng-binding message-time" style="font-size:12px;font-weight:400;vertical-align: -webkit-baseline-middle;"></span>
                    <div class="bubble js_message_bubble ng-scope bubble_primary right" ng-class="{&quot;bubble_default left&quot;:!message.MMIsSend,&quot;bubble_primary right&quot;:message.MMIsSend, &quot;arrow_primary&quot;: message.MsgType == CONF.MSGTYPE_APP &amp;&amp; message.AppMsgType == CONF.APPMSGTYPE_URL, &quot;no_arrow&quot;: message.MsgType == CONF.MSGTYPE_IMAGE || message.MsgType == CONF.MSGTYPE_MICROVIDEO || message.MsgType == CONF.MSGTYPE_VIDEO}" data-cm="{&quot;type&quot;:&quot;message&quot;,&quot;actualSender&quot;:&quot;@1a72dcdadcc4d5b23145f8b689c2a40e&quot;,
                         &quot;msgType&quot;:&quot;1&quot;,&quot;subType&quot;:0,&quot;msgId&quot;:&quot;4392033870685675206&quot;}">
                        <div class="bubble_cont ng-scope" >
                            <div class="plain"><pre class="js_message_plain ng-binding" >请输入你的用户名</pre>
                                <!--<img class="ico_loading " src="//res.wx.qq.com/a/wx_fed/webwx/res/static/img/25x4Rho.gif" alt="">-->
                                <!--<i class="ico_fail web_wechat_message_fail ng-hide" ng-click="resendMsg(message)" ng-show="message.MMStatus == 5" title="重新发送"></i>-->
                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>
</div>

<div id="chat-item-you-template" class="ng-scope" style="display:none">
