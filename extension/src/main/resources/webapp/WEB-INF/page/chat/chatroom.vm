<script type="text/ng-template" id="navChat.html">
    <!--BEGIN chat list-->
    <div jquery-scrollbar class="chat_list scrollbar-dynamic" id="J_NavChatScrollBody">
        #foreach($robot in $robots)
            <div  data-height="64" data-buffer-height="200" >
                <div class="chat_item slide-left" data-username="$robot.name" data-userno="$robot.services" data-cm='{"type":"chat","username":"$robot.name"}'>
                    <!--<div class="ext"><p class="attr">16:00</p></div>-->
                    <div class="avatar">
                        <img class="img" src="$robot.logo" alt=""/>
                        <!--<i class="icon web_wechat_reddot" ></i>-->
