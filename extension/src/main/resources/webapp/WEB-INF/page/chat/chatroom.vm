            </div>
        #end
    </div>
    <!--END chat list-->
</script>

<script type="text/ng-template" id="contentChat.html">
    <div id="chat-area" ng-class="{'chatRoom':currentContact.isRoomContact(),'no-choose':!currentContact.getDisplayName()}" class="box chat ng-scope chatRoom">

        <!--BEGIN HD-->
        <div class="box_hd">
            <!--<a class="ext web_wechat_addfriend" href="javascript:;" ng-click="showChatRoomMembers($event)" ng-show="currentContact.isRoomContact()" title="成员"></a>-->
            <!--<a class="ext web_wechat_addfriend" href="javascript:;" ng-click="showProfile($event)" ng-show="!currentContact.isRoomContact()" title="Profile"></a>-->
            <div id="chatRoomMembersWrap"></div>
            <div class="title_wrap">
                <div class="title poi" ng-click="currentContact.MMCanCreateChatroom &amp;&amp; showChatRoomMembers($event)">
                    <a class="title_name ng-binding" data-username="@@58c5738a8c2a1d2ef3de7f6e4b4128c03483fe2fc2b940897ab2b45a8d5fefd5"></a>
                    <!--<span class="title_count ng-binding ng-scope" >(17)</span>-->
