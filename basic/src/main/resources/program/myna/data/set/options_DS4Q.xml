<dataset>
  <module>config</module>
  <code>options_DS4Q</code>
  <entity>options</entity>
  <name>选项集【查询】</name>
  <fields>
    <field code="id" name="选项集id" edit-type="hidden" is-key="true" not-null="true"/>
    <field code="code_LKE" name="选项集编码" edit-type="input" not-null="true"/>
    <field code="name_LKE" name="选项集名称" edit-type="input" not-null="true"/>
    <field code="status" name="状态" edit-type="select">
      <enum-class code="options.status"/>
    </field>
    <field code="creator_id" name="创建人" edit-type="hidden">
      <rel entity-code="user/user_id/user_name" add-by-global="true"/>
    </field>
    <field code="create_time" name="创建时间" edit-type="hidden"/>
    <field code="modifier_id" name="修改人" edit-type="hidden">
      <rel entity-code="user/user_id/user_name" add-by-global="true"/>
    </field>
    <field code="modify_time" name="修改时间" edit-type="hidden"/>
  </fields>
</dataset>
